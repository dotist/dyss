{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": [
    "react", "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2019,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
   "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "prettier/prettier": "error"
  },
   "settings": {
     "react": {
      "version": "detetect"
    }
  }
}
